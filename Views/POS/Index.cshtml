        function showProductCategory() {
            //var pid = $('#pid').val();
            //alert("topup");
            $.ajax({
                url: '@Url.Action("getProductCategoryForIndex", "Product")',
                //data: { ptype: "TopUp", filter: "%", cat: "", brand: "", firstUOM: 0 },
                type: 'POST',
                success: function (data) {
                    //if (data)
                    //alert(data);
                    populateProductCategory(data);
                },
                error: function (ts) { alert(ts.responseText) }
            });
        }

        function populateProductCategory(data) {
            //divProduct
            var Htmlstr = "";
            for (var x = 0; x < data.length; x++) {
                if (data[x].Category != "Package") {
                    Htmlstr += "<div class='col-sm-4'><a class='quick-button' href='../Product/ProductListing?rcode=PRODUCT&ptype=" + data[x].Category + "'><i class='fa fa-list-alt'></i><p>" + data[x].Category + "</p></a> </div>";
                    if (((x + 1) % 3 == 0) && x != 0) Htmlstr += "<div class='clearfix'></div><div class='col-sm-12'  style='min-height: 10px;'></div>";
                    //if (((x + 1) % 3 == 0) && x != 0) Htmlstr += "<div class='clearfix'><div class='col-sm-12' style='min-height:10px;'></div>";
                }
            }
            $('#divProduct').html(Htmlstr);

        }
